@startuml
!include style.puml

Actor User as user USER_COLOR
Participant ":EYLAH" as ui UI_COLOR
Participant ":LogicManager" as logic LOGIC_COLOR
Participant ":ModelManager" as model MODEL_COLOR
Participant ":FoodBook" as FoodBook MODEL_COLOR

user -[USER_COLOR]> ui : "list"
activate ui UI_COLOR

ui -[UI_COLOR]> logic : execute("list")
activate logic LOGIC_COLOR

logic -[LOGIC_COLOR]> model : updateFilteredFoodList()
activate model MODEL_COLOR

model -[MODEL_COLOR]> FoodBook : toString()
activate FoodBook MODEL_COLOR

FoodBook --[STORAGE_COLOR]> model : String
deactivate FoodBook

model --[MODEL_COLOR]-> logic : String
deactivate model

logic --[LOGIC_COLOR]> ui : CommandResult
deactivate logic

ui--[UI_COLOR]> user : CommandResult
deactivate ui
@enduml
