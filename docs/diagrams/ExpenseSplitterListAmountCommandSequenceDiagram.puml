@startuml
!include style.puml

Actor User as user USER_COLOR
Participant ":EYLAH" as ui UI_COLOR
Participant ":LogicManager" as logic LOGIC_COLOR
Participant ":ModelManager" as splitterModel MODEL_COLOR
Participant ":PersonAmountBook" as personAmountBook MODEL_COLOR

user -[USER_COLOR]> ui : "listamount"
activate ui UI_COLOR

ui -[UI_COLOR]> logic : execute("listamount")
activate logic LOGIC_COLOR

logic -[LOGIC_COLOR]> splitterModel : listAmount()
activate splitterModel MODEL_COLOR

splitterModel -[MODEL_COLOR]> personAmountBook : toString()
activate personAmountBook MODEL_COLOR

personAmountBook --[STORAGE_COLOR]> splitterModel : String
deactivate personAmountBook

splitterModel --[STORAGE_COLOR]-> logic : String
deactivate splitterModel

logic --[LOGIC_COLOR]> ui : CommandResult
deactivate logic

ui--[UI_COLOR]> user : CommandResult
deactivate ui
@enduml
